{
  "name": "sort-by-property",
  "version": "0.4.0",
  "description": "Type-safe array sorting method with support for deeply nested properties and Typescript autocompletion.",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "devDependencies": {
    "typescript": "^4.8.4",
    "@types/jest": "^29.2.2",
    "@types/node": "^18.11.9",
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "@typescript-eslint/parser": "^5.42.1",
    "auto-changelog": "^2.4.0",
    "eslint": "^8.27.0",
    "husky": "^8.0.2",
    "jest": "^29.3.0",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "release-it": "^15.5.0",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.0"
  },
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "example": "npx ts-node ./src/examples/nested-property.ts",
    "prepare": "husky install",
    "release": "release-it --changelog",
    "lint-staged": "lint-staged"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jvandenaardweg/sort-by-property.git"
  },
  "keywords": [
    "typescript",
    "sorting",
    "array",
    "sort",
    "sortby",
    "javascript",
    "order",
    "orderby",
    "typed",
    "typesafe"
  ],
  "author": "Jordy van den Aardweg <jordyvandenaardweg@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jvandenaardweg/sort-by-property/issues"
  },
  "homepage": "https://github.com/jvandenaardweg/sort-by-property#readme",
  "release-it": {
    "git": {
      "commitMessage": "chore: release v${version}",
      "changelog": "npx auto-changelog --stdout --commit-limit false --unreleased --template https://raw.githubusercontent.com/release-it/release-it/master/templates/changelog-compact.hbs"
    },
    "hooks": {
      "after:bump": "npx auto-changelog -p"
    },
    "npm": {
      "publish": false
    },
    "github": {
      "release": true,
      "releaseName": "v${version}"
    }
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "yarn test-single"
    ],
    "*.js": [
      "prettier --write"
    ],
    "*.json": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "precommit": "lint-staged"
    }
  }
}
